{
  "manifest_version": 2,

  "name": "Id cookie reader",
  "description": "Fetch data from the id cookie.",
  "version": "3.0",

  "icons": {
    "90": "icon.png"
  },

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*/*",
    "activeTab",
    "storage",
    "cookies"
  ]
}
